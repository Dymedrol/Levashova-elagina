    <div class="le-header-dropdown" id="header-dropdown">
        <div class="container le-header-dropdown__inner">
            <div class="le-header-dropdown-menu">
                <div class="le-header-dropdown-menu-mobile">
                    <div class="le-header-dropdown-menu-mobile-header">
                        {% if linklists.top-menu-1 %}<div class="le-header-dropdown-menu-mobile-header__item le-header-dropdown-menu-mobile-header__item_active">{{ linklists.top-menu-1.title }}</div>{% endif %}
                        {% if linklists.top-menu-2 %}<div class="le-header-dropdown-menu-mobile-header__item">{{ linklists.top-menu-2.title }}</div>{% endif %}
                        {% if linklists.top-menu-3 %}<div class="le-header-dropdown-menu-mobile-header__item">{{ linklists.top-menu-3.title }}</div>{% endif %}
                    </div>
                    {% if linklists.top-menu-1 %}
                    <div class="le-header-dropdown-menu-mobile-body">
                        {% for link in linklists.top-menu-1.links %}
                            <a href="{{ link.url }}" class="le-header-dropdown-menu-mobile-body__item">{{ link.title }}</a>
                        {% endfor %}
                    </div>
                    {% endif %}
                    {% if linklists.top-menu-2 %}
                    <div class="le-header-dropdown-menu-mobile-body">
                        {% for link in linklists.top-menu-2.links %}
                            <a href="{{ link.url }}" class="le-header-dropdown-menu-mobile-body__item">{{ link.title }}</a>
                        {% endfor %}
                    </div>
                    {% endif %}
                    {% if linklists.top-menu-3 %}
                    <div class="le-header-dropdown-menu-mobile-body">
                        {% for link in linklists.top-menu-3.links %}
                            <a href="{{ link.url }}" class="le-header-dropdown-menu-mobile-body__item">{{ link.title }}</a>
                        {% endfor %}
                    </div>
                    {% endif %}
                </div>
                <div class="le-header-dropdown-menu-desktop">
                    {% if linklists.top-menu-1 %}
                        <div class="le-header-dropdown-menu-desktop-col">
                            <div class="le-header-dropdown-menu-desktop__title">{{ linklists.top-menu-1.title }}</div>
                            <div class="le-header-dropdown-menu-desktop-list">
                                {% for link in linklists.top-menu-1.links %}
                                    <a href="{{ link.url }}" class="le-header-dropdown-menu-desktop-list__item">{{ link.title }}</a>
                                {% endfor %}
                            </div>
                        </div>
                    {% endif %}
                    {% if linklists.top-menu-2 %}
                        <div class="le-header-dropdown-menu-desktop-col">
                            <div class="le-header-dropdown-menu-desktop__title">{{ linklists.top-menu-2.title }}</div>
                            <div class="le-header-dropdown-menu-desktop-list">
                                {% for link in linklists.top-menu-2.links %}
                                    <a href="{{ link.url }}" class="le-header-dropdown-menu-desktop-list__item">{{ link.title }}</a>
                                {% endfor %}
                            </div>
                        </div>
                    {% endif %}
                    {% if linklists.top-menu-3 %}       
                        <div class="le-header-dropdown-menu-desktop-col">
                            <div class="le-header-dropdown-menu-desktop__title">{{ linklists.top-menu-3.title }}</div>
                            <div class="le-header-dropdown-menu-desktop-list">
                                {% for link in linklists.top-menu-3.links %}
                                    <a href="{{ link.url }}" class="le-header-dropdown-menu-desktop-list__item">{{ link.title }}</a>
                                {% endfor %}
                            </div>
                        </div>
                    {% endif %}
                </div>
            </div>
            <div class="le-header-dropdown-banner"{% if widget_settings.banner_image %} style="background-image: url('{{ widget_settings.banner_image | image_url: 1100, format: 'webp', resizing_type: 'fit_width' }}');"{% endif %}>
                <div class="le-header-dropdown-banner__text">{{ widget_settings.banner_text | default: 'Banner text' }}</div>
            </div>
        </div>
    </div>
